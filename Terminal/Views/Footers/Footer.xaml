<Page x:Class="Terminal.Views.Footers.Footer"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Terminal.Views.Footers"
      mc:Ignorable="d" 
      d:DesignHeight="102.4" d:DesignWidth="1280"
      Title="Footer">

    <Grid Name="footerGrid" Background="#003F72">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="350"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="256"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="350"/>
        </Grid.ColumnDefinitions>
        <Frame Grid.Column="2" x:Name="LenguageFrame">
            
        </Frame> 
        <Image Grid.Column="0" Source="\Images\Footers\Moons.png" Margin="20"></Image>
        <Image Grid.Column="1" Source="\Images\Footers\MoonsContact.png" Margin="5"></Image>
        <Image Grid.Column="3" Source="\Images\Footers\MoonsLicens.png" Margin="5"></Image>
        <Image Grid.Column="4" Source="\Images\Footers\PaywayLogo.png" Margin="20"></Image>
    </Grid>
</Page>
